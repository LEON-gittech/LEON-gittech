// 回文数.cpp : 此文件包含 "main" 函数。程序执行将在此处开始并结束。
//
#include <iostream>
#include<set>
#include<algorithm>
#include<cstring>
#include<string>
#include<cstdio>
#include<ctype.h>
#include<cmath>
#include<vector>
#include<map>
#include<queue>
#include<stack>
#define re register
#define lson o<<1
#define rson o<<1|1
int const inf = 0x3f3f3f3f;
typedef long long ll;
using namespace std;
set<int>::const_iterator iter;
int const maxn = 100000;
const double eps = 1e-6;
const ll n = 2021041820210418;
ll cnt = 0;


int main()
{
    vector<ll> a;

    for (ll i = 1; i * i <= n; i++)
    {
        if (n % i == 0)
        {
            a.push_back(i);
            a.push_back(n / i);
        }
    }

    int res = 0;
    for (auto i : a)
        for (auto j : a)
            for (auto k : a)
                if (i * j * k == n) res++;//通过先计算约数进而简化运算

    cout << res << endl;

    return 0;
}
